{
  "manifest_version": 3,
  "name": "slide_bandit",
  "version": "69.420",
  "description": "stealing pixels by oio",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab", "downloads", "notifications", "scripting"],
  "host_permissions": ["https://docs.google.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/presentation/*"],
      "js": ["content.js"]
    }
  ]
}
